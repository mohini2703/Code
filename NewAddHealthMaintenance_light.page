    function fnSetDateFormat(oDateFormat)
    { 
     oDateFormat['FullYear'];  //Example = 2007
     
     oDateFormat['Year'];   //Example = 07
     oDateFormat['FullMonthName']; //Example = January
     oDateFormat['MonthName'];  //Example = Jan
     oDateFormat['Month'];   //Example = 01
     oDateFormat['Date'];   //Example = 01
     oDateFormat['FullDay'];   //Example = Sunday
     oDateFormat['Day'];    //Example = Sun
     oDateFormat['Hours'];   //Example = 01
     oDateFormat['Minutes'];   //Example = 01
     oDateFormat['Seconds'];   //Example = 01
      
     var sDateString;
      
     // Use dd/mm/yyyy format
     sDateString = oDateFormat['Month'] +"/"+ oDateFormat['Date'] +"/"+ oDateFormat['FullYear']; 
     return sDateString;
     
    }
   
     
    function initialiseCalendar(obj, eleId)
    {
     var element=document.getElementById(eleId);
     var params='close=true';
     if (null!=element)
     {
      if (element.value.length>0)
      {
       // date is formatted dd/mm/yyyy - pull out the month and year
       var month=element.value.substr(3,2);
       var year=element.value.substr(6,4);
       params+=',month='+month;
       params+=',year='+year;
      }
     }
     fnInitCalendar(obj, eleId, params);
    }

    function checkBoxClick(checkBoxGroup,clickElement){
        try{
            $('input.'+checkBoxGroup).not(clickElement).prop('checked', false);
            window[checkBoxGroup]($(clickElement).prop("checked"),clickElement.value);
        }
        catch(error){
            console.log('exception' + error);
        }
    }
    
   /**
        Method to verify Previous Flu vaccination date is greater than new entered Flu vaccination date.
   **/
   function isFluVaccineDateLessThanPrevFluDate(){
        var preValue = $( "input[name$='preFlueDate']" ).val().split("/");
        var preFluDate = new Date(preValue[2], preValue[0]-1, preValue[1]);
        var newValue = $( "input[name$='Dobdate']" ).val().split("/");
        var newFluDate = new Date(newValue[2], newValue[0]-1, newValue[1]);
        
        if(preFluDate.getTime() > newFluDate.getTime()){
            var proceed = confirm("The date entered is less than the previous date. Do you want to continue?");
            if(proceed){
                validateHMForm();
            }
        }
        else{
            validateHMForm();
        }
   }
    
   function validateFluDate(){
        var preValue = $( "input[name$='preFlueDate']" ).val().split("/");
        var preFluDate = new Date(preValue[2], preValue[0]-1, preValue[1]);
        var newValue = $( "input[name$='Dobdate']" ).val().split("/");
        var newFluDate = new Date(newValue[2], newValue[0]-1, newValue[1]);
        
        if(preFluDate.getTime() > newFluDate.getTime()){
            var proceed = confirm("The date entered is less than the previous date. Do you want to continue?");
            if(proceed){
                saveAndApproveActionFun();
            }
        }
        else{
            saveAndApproveActionFun();
        }
   }
    
   function validateHMForm(){
        var isValid = true;
        var apId = '{!ApId}';
        var skipPreFlueField = false;
        if(apId != ''){
            $(".class-forJs input[type=\"text\"]").each(function() {
               if(skipPreFlueField){
                   var element = $(this);
                   if (element.val() == "") {
                       isValid = false;
                   }
                   else{
                      isValid = true;
                       return false;
                   }    
               }
               else{
                    skipPreFlueField = true;
               }
               
            });
            if(!isValid){
                $("textarea").each(function() {
                   var element = $(this);
                   if ($.trim(element.val()) == "") {
                       isValid = false;
                   }
                   else{
                      isValid = true;
                      return false;
                   }
                });
            }
            if(!isValid){
                $(".class-forJs input[type=\"checkbox\"]").each(function() {
                    var element1 = $(this);
                    //if(element1.is(':checked')){
                        isValid = true;
                    //}
                });
            }
            if(!isValid){
                 var isValid = {!if(lstA1CResult!=null && lstA1CResult.size>0,true,false)};
            }
            
            if(!isValid){
                 var isValid = {!if(lstLDLResult!=null && lstLDLResult.size>0,true,false)};
            }
        
        } 
        
        if(isValid){
            saveAndSubmitHMActionFun();
        }
        else{
            alert('No modification has been made for this record.');
        }
    }
</script>
